page{
  background: #ffffff;
}
.banner image{
width:100%;
height: 100%;

}
.banner{
width: 690rpx;
height: 220rpx;
background: linear-gradient(180deg, #0660FE 0%, #2C99FE 100%);
border-radius: 0px 30rpx 0px 30rpx;
overflow: hidden; 
}
.srv-step{
  padding: 0 5rpx;
}
.stepcard>view{ 
min-height: 255rpx;
background: #F5F7F8;
border-radius: 8rpx;
margin:15rpx 25rpx 25rpx 25rpx;
padding:35rpx 10rpx;
}
.stepcard>view>view:first-child image{
  width: 90rpx;
  height: 90rpx;
}
