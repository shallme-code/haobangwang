/* pages/template/answer/index.wxss */
.answer-card{
  padding: 30rpx 30rpx 20rpx 80rpx;
  position: relative;
  background: #F9FAFB;
  margin-top:30rpx;
  border-radius: 8rpx;
}
.answer-card .q-icon{
  position: absolute;
  top: 30rpx;
  left:16rpx;
}
.answer-card .q-icon,.answer-card .q-icon image{
  width: 40rpx;
  height: 40rpx;
}
.answer-card>view:last-child{
   line-height: 50rpx;
   margin-top: 20rpx;
}
