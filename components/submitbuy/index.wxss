/* components/btnbuy/index.wxss */
.btnbuy {
  position: fixed;
  z-index: 1000;
  bottom: 0;
  right: 0;
  width: 100%;
  height: 120rpx;
  background: #FFFFFF;
  box-shadow: 0px -2rpx 8rpx 0rpx rgba(217, 217, 217, 0.3);
  display: flex;
  flex-direction: row;
  padding: 0 30rpx;
  align-items: center;
  justify-content: space-between
}

.btnbuy>view:first-child image {
  width: 40rpx;
  height: 40rpx;
}