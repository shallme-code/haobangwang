@font-face {
  font-family: 'Source Han Sans CN';
  font-display: swap;
  src: url('//at.alicdn.com/t/webfont_d9gzu0tg0e.eot');
  /* IE9*/
  src: url('//at.alicdn.com/t/webfont_d9gzu0tg0e.eot?#iefix') format('embedded-opentype'),
    /* IE6-IE8 */
    url('//at.alicdn.com/t/webfont_d9gzu0tg0e.woff2') format('woff2'),
    url('//at.alicdn.com/t/webfont_d9gzu0tg0e.woff') format('woff'),
    /* chrome、firefox */
    url('//at.alicdn.com/t/webfont_d9gzu0tg0e.ttf') format('truetype'),
    /* chrome、firefox、opera、Safari, Android, iOS 4.2+*/
    url('//at.alicdn.com/t/webfont_d9gzu0tg0e.svg#NotoSansHans-DemiLight') format('svg');
  /* iOS 4.1- */
}
